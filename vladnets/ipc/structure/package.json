{
  "name": "@vladnets/ipc-structure",
  "version": "1.0.0-a",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "private": true,
  "scripts": {
    "compile": "webpack --colors --env.name=main",
    "compile:watch": "webpack --watch --colors --env.name=main",
    "start": "nodemon -q -e ts -w ./dist/main.js dist/main.js",
    "start:watch": "yarn run compile && concurrently -n \"index,app\" \"yarn run compile:watch\" \"yarn start\""
  },
  "devDependencies": {
    "@types/node": "^10.10.1",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4",
    "ts-loader": "^5.1.1",
    "typescript": "^3.0.3",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0"
  }
}
